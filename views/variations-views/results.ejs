<h2> Search Results </h2>

<form method="get" action="/products/search-results">
  <label for="term-input"> Search again </label>
  <input  id="term-input" name="searchTerm" type="search"
      value="<%= lastSearch %>">

  <button> Search </button>
</form>

<p> You searched for <strong><%= lastSearch %></strong>. </p>
<p> There were <%= listOfResults.length %> results. </p>

<ul>
  <% listOfResults.forEach((oneProduct) => { %>
    <li>
      <h3> <%= oneProduct.name %> </h3>
      <p> Price: <%= oneProduct.price %> </p>

      <a href="/products/<%= oneProduct._id %>">
        Learn More
      </a>
    </li>
  <% }) %>
</ul>
